<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.appmonchis">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Cena" android:parentActivityName=".MenuPrincipalActivity"></activity>
        <activity android:name=".Almuerzo" android:parentActivityName=".MenuPrincipalActivity"/>
        <activity android:name=".Desayuno" android:parentActivityName=".MenuPrincipalActivity"/>
        <activity
            android:name=".MenuRegistro" android:parentActivityName=".Login"
            android:label="Regristro"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MenuPrincipalActivity" />
        </activity>
        <activity

            android:name=".MenuPrincipalActivity"
            android:label="Monchis Time"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Login"
            android:label="Entrar" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="Monchis Time"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>